<%= simple_form_for @party, url: "/bartenders/search" do |f| %>
  <%= f.input :address, label: false, placeholder: 'Location', wrapper_html: { class: '' }, input_html: { class: "input form-input-index" } %>

  <div class="">
    <%= f.input :theme, collection: Party::TYPES, label: false, prompt: "Type of Party", input_html: { class: "input form-input-index" } %>
  </div>

  <div class="">
    <%= f.input :size, collection: ['5-29', '30-59', '60-90'], label: false, prompt: 0, input_html: { class: "input form-input-index" } %>
  </div>

  <div class="">
    <%= f.input :date, as: :datepicker, label: false, placeholder: 'Please select date', input_html: {class: "datep"} %>
  </div>

  <div class="">
    <%= button_tag type: :submit, class: "btn-search", id: 'btn-search-index' do %>
      <i class="fa fa-search"></i>
    <% end %>
  </div>
<% end %>

<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js">

  function initialize() {

  var input = document.getElementById('party_address');
  var autocomplete = new google.maps.places.Autocomplete(input);
  }

  $(document).ready(function() {
  // the DOM is now ready, you can traverse it,
  // and register events with on().
  console.log("test");
  $(".datep").placeholder= "date";
  })


  google.maps.event.addDomListener(window, 'load', initialize);
</script>




