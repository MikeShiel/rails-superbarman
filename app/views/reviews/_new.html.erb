<%= simple_form_for [@bartender, @review] do |f| %>
<%= f.input :content, label: "How was your experience?", required: false, placeholder: "Awesome!", input_html: { class: "button-add-review" }%>
<br>
<%= f.input :rating, collection: 0..5, label: "Rate it!", required: false, selected: 5, input_html: { class: "button-add-review" }%>
<br><br>
<%= f.button :submit, :class => "button-add-review"%>
<% end %>



